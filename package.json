{
  "name": "snapcase-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "vercel-build": "next build",
    "verify:mcp": "node scripts/verify-mcp.js",
    "test:unit": "npx --yes jest --runInBand --testPathPatterns tests/unit",
    "test:integration": "npx --yes jest --runInBand --testPathPatterns tests/integration",
    "test:e2e": "npx --yes playwright test",
    "verify": "npm run verify:mcp && npm run test:unit && npm run test:integration && npm run test:e2e"
  },
  "dependencies": {
    "next": "^14.2.33",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.48.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "mcp-client": "^1.13.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
